
<div class="row">
  <div class="col-xs-12">
    <div id="chart"></div>
  </div>
</div>
<br>
<hr>
<script>
  $(function () {
    $('#chart').highcharts({
      chart: {
        type: 'column'
      },
      title: {
        text: '<%= @benchmark.name.capitalize %> results'
      },
      xAxis: {
        type: 'category',
        title: {
          text: 'Versions'
        }
      },
      yAxis: {
        type: 'logarithmic',
        title: {
          text: 'Time (second)'
        },
//        min: 0
      },
      plotOptions: {
        line: {
          dataLabels: {
            enabled:  true
          },
          enableMouseTracking: false
        }
      },
      tooltip: {
        pointFormat: '<b>{point.y:,.4f}</b> sec'
      },
      series: [
        <% @results.keys.sort.each do |gcc_version| %>
        <% results = @results[gcc_version] %>
        {
//          upColor: Highcharts.getOptions().colors[1],
//          color: Highcharts.getOptions().colors[2],
          name: '<%= gcc_version %>',
          // Define the data points. All series have a dummy year
          // of 1970/71 in order to be compared on the same x axis. Note
          // that in JavaScript, months start at 0 for January, 1 for February etc.
          data: [
            <% results.keys.sort.each do |version| %>
            <% data = results[version] %>
            {
              name: '<%= version %>',
              y: <%= data[:average] %>,
//              color: Highcharts.getOptions().colors[<%= RubyVersion::IMPLEMENTATIONS.values.index data[:implementation] %>]
            },
            <% end %>
          ]
        },
        <% end %>
      ],
    });
  });
</script>